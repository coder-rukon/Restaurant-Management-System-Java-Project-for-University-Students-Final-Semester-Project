<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>

<AnchorPane prefHeight="540.0" prefWidth="707.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="customer.My_Orders">
   <children>
      <TableView fx:id="viewOrder" layoutX="47.0" layoutY="77.0" prefHeight="344.0" prefWidth="661.0">
        <columns>
          <TableColumn fx:id="order_id" prefWidth="75.0" text="Order Id" />
          <TableColumn fx:id="product_name" prefWidth="251.0" text="Product Name" />
            <TableColumn fx:id="order_price" prefWidth="104.0" text="Price" />
            <TableColumn fx:id="order_quantity" prefWidth="104.0" text="Quantity" />
            <TableColumn fx:id="order_date" prefWidth="104.0" text="Date" />
        </columns>
      </TableView>
      <Pane layoutX="26.0" layoutY="24.0" prefHeight="53.0" prefWidth="525.0">
         <children>
            <Button fx:id="btn_find_foods" layoutX="23.0" layoutY="14.0" mnemonicParsing="false" onAction="#displayFindFoods" text="Find Foods" />
            <Button fx:id="btn_my_order" layoutX="105.0" layoutY="14.0" mnemonicParsing="false" onAction="#displayMyOrders" text="My Orders" />
         </children>
      </Pane>
   </children>
</AnchorPane>
