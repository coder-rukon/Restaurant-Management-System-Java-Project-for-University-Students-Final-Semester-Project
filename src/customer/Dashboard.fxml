<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>

<AnchorPane prefHeight="624.0" prefWidth="618.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="customer.Dashboard">
   <children>
      <Pane layoutX="16.0" layoutY="14.0" prefHeight="53.0" prefWidth="525.0">
         <children>
            <Button fx:id="btn_find_foods" layoutX="23.0" layoutY="14.0" mnemonicParsing="false" text="Find Foods" />
            <Button fx:id="btn_my_order" layoutX="105.0" layoutY="14.0" mnemonicParsing="false" onAction="#displayMyOrders" text="My Orders" />
         </children>
      </Pane>
      <TableView fx:id="viewFoods" layoutX="14.0" layoutY="75.0" prefHeight="406.0" prefWidth="525.0">
        <columns>
          <TableColumn fx:id="FoodNameColumn" prefWidth="401.0" text="Food Name" />
          <TableColumn fx:id="PriceColumn" minWidth="0.0" prefWidth="130.0" text="Price" />
        </columns>
      </TableView>
   </children>
</AnchorPane>
